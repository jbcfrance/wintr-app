<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Title -->
    <title>{% if title is defined %}{{ title }}{% else %} Wintr Application {% endif %}</title>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>

<body>
{% block body %}
{% endblock %}

{% block javascript %}
{{ encore_entry_script_tags('app') }}

{% endblock %}
</body>
</html>
